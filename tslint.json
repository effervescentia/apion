{
  "extends": [
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "ordered-imports": [
      true,
      {
        "import-sources-order": "lowercase-last",
        "named-imports-order": "lowercase-first"
      }
    ],
    "member-ordering": [
      true,
      {
        "order": "instance-sandwich"
      }
    ],
    "import-name": false
  }
}
